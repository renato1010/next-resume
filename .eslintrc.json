{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:import/recommended"
  ],
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": [2, { "commonjs": true, "amd": true }],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2
  }
}
